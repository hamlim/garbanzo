{
  "name": "@garbanzo/cli",
  "version": "0.0.1",
  "description": "The Garbanzo CLI",
  "homepage": "TODO",
  "author": {
    "name": "Matt Hamlin",
    "email": "matthewjameshamlin@gmail.com"
  },
  "bin": {
    "garbanzo": "./cli.mjs"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hamlim/garbanzo",
    "directory": "packages/cli"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": ["dist", "CHANGELOG.md", "README.md", "cli.mjs"],
  "type": "module",
  "devDependencies": {
    "@swc/cli": "0.3.10",
    "@swc/core": "1.4.2",
    "@types/bun": "1.1.11",
    "hohoro": "0.3.0",
    "typescript": "5.4.5"
  },
  "scripts": {
    "dev": "node --watch-path=./src dev.mjs",
    "build": "hohoro-experimental",
    "type-check": "tsc -p ./tsconfig.json",
    "lint": "biome lint ./src/",
    "test": "bun test",
    "prepub": "bun run build && bun run type-check && bun run lint && bun run test",
    "pub": "npm publish --access public"
  },
  "dependencies": {
    "giget": "^1.2.4",
    "mri": "^1.2.0"
  }
}
